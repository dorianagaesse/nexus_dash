NODE_ENV=development
APP_PORT=3000

# Google OAuth / Calendar
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
# Set when Google OAuth is enabled (must be configured with GOOGLE_CLIENT_ID + GOOGLE_CLIENT_SECRET).
GOOGLE_REDIRECT_URI=
GOOGLE_CALENDAR_ID=primary

# Optional, depending on auth strategy
NEXTAUTH_URL=
NEXTAUTH_SECRET=

# Optional Supabase REST/client configuration
SUPABASE_URL=
SUPABASE_PUBLISHABLE_KEY=

# Database (PostgreSQL / Supabase)
DATABASE_URL=postgresql://<user>:<password>@<pooler-host>:5432/postgres?sslmode=require
# Required in production. In local/dev, may match DATABASE_URL if direct host is unavailable.
DIRECT_URL=postgresql://<user>:<password>@<direct-host>:5432/postgres?sslmode=require
